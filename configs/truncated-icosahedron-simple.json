{
    "map_data_spec": [
        {
            "fname": "data/continents.geo.json",
            "pmap": {"name": "CONTINENT", "continent": "CONTINENT"},
            "count_threshold": 0,
            "area_threshold_km2": 100000,
            "downsample_rate": 1,
            "name_whitelist": []
        }
