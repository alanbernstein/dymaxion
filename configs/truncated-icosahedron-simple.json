    "projection": {
        "polyhedron": "truncated-icosahedron",
        "method": "simple",
        "rotation": "poles-at-pentagons",
        "circumradius_in": 8
    },
    "svg_filename": "svg/truncated-icosahedron-simple.svg",
    "svg": true,
    "plot2d": true,
    "plot3d": true
}
