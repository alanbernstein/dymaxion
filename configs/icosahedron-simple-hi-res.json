{
    "map_data_spec": [
        {
            "fname": "data/world-110m.geo.json",
            "pmap": {"name": "name", "continent": "continent"},
            "count_threshold": -1,
            "area_threshold": 10000,
            "downsample_rate": 10,
            "name_whitelist": ["Canada", "United States", "Australia", "China", "Egypt", "Brazil", "Germany", "Russia", "Antarctica"]
        }
    ],
    "projection": {
        "polyhedron": "icosahedron",
        "method": "simple",
        "rotation": "poles-at-vertices"
    },
    "svg_filename": "svg/icosahedron-simple-hires.svg",
    "svg": true,
    "plot2d": true,
    "plot3d": true
}
